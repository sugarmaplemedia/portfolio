---
import GalleryStatic from "./GalleryStatic.svelte"
import data from "../databases/galleries.json"

interface Props {
	dbListName: keyof typeof data
	size: "sml" | "md" | "lg"
	tag: string
	count: number
}

const { dbListName, size = "md", tag = "", count = 0 } = Astro.props

const dbList = data[dbListName]
---

<GalleryStatic database={dbList} {size} {tag} {count} />
